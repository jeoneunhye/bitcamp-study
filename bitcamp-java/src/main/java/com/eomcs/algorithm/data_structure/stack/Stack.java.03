package com.eomcs.algorithm.data_structure.stack;
// 3) 레퍼런스 배열을 초기화시키는 생성자를 만든다.
public class Stack {
  private static final int DEFAULT_CAPACITY = 10;
  
  Object[] elementData;
  int size;
  
  public Stack() {
    this.elementData = new Object[DEFAULT_CAPACITY];
    this.size = 0; // 생략 가능
  }
}