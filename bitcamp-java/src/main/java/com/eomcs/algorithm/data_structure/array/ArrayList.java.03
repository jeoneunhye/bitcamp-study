package com.eomcs.util;

public class ArrayList {
  Object[] elementData; // 아직 레퍼런스가 null인 상태
  int size;
  
  public ArrayList() {
    super();
    
    this.elementData = new Object[100]; // 100개의 객체(의 주소)를 저장할 수 있는 배열을 생성(초기화)
    // size = 0; 인스턴스 변수는 생성될 때 0으로 초기화되므로 굳이 생성자 안에서 size의 값을 0으로 초기화시키지 않아도 된다.
  }
}